<div class="container">
    <div class="jumbotron">
        <div class="container">
        <h1>All Wikis</h1>
        <% @wikis.each do |wiki| %>
          <div class="media col-sm-4">
             <div class="media-body">
               <h4 class="media-heading">
                 <span><%= link_to wiki.title, wiki %></span>
               </h4>
             </div>
           </div>
        <% end %>
        </div>
    </div>
</div>